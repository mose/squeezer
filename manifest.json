{
  "name": "Snapshot",
  "description": "Snapshot the good part of your page",
  "background": {
    "scripts": ["popup.js"]
  },
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    // "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
